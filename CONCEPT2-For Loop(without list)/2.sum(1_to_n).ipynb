   "source": [
    "Given a positive integer, find out the sum of all integers from 1 up to this number."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "# take a number as input from user\n",
    "def func_sum(n):\n",
    "    sum=0\n",
    "    for x in range(1,n+1):\n",
    "        sum=sum+x\n",
    "    return sum\n"
   ]
